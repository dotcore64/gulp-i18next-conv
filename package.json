{
  "name": "gulp-i18next-conv",
  "version": "1.1.0",
  "description": "Gulp plugin to convert gettext .po or .mo files to i18next compatible JSON",
  "main": "dist/index.js",
  "scripts": {
    "test": "gulp test",
    "build": "gulp build",
    "cover": "istanbul cover --report text --report html node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrin4869/gulp-i18next-conv.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "i18next",
    "gettext",
    "json"
  ],
  "author": "Julian Grinblat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perrin4869/gulp-i18next-conv/issues"
  },
  "homepage": "https://github.com/perrin4869/gulp-i18next-conv#readme",
  "dependencies": {
    "gulp-util": "^3.0.7",
    "i18next-conv": "^3.0.3",
    "pkginfo": "^0.4.0",
    "through2": "^2.0.1",
    "vinyl-contents-tostring": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.13.0",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.2",
    "rewire": "^2.5.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "vinyl": "^1.2.0"
  }
}
