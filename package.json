{
  "name": "gulp-i18next-conv",
  "version": "4.0.1",
  "description": "Gulp plugin to convert gettext .po or .mo files to i18next compatible JSON",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint 'src/*.js' 'test/*.js'",
    "pretest": "npm run lint",
    "test": "mocha",
    "build": "babel src -d dist",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm run test && npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrin4869/gulp-i18next-conv.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "i18next",
    "gettext",
    "json"
  ],
  "author": "Julian Grinblat",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/perrin4869/gulp-i18next-conv/issues"
  },
  "homepage": "https://github.com/perrin4869/gulp-i18next-conv#readme",
  "dependencies": {
    "gulp-util": "^3.0.8",
    "i18next-conv": "^6.0.0",
    "pkginfo": "^0.4.1",
    "through2": "^2.0.3",
    "vinyl-contents-tostring": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "dirty-chai": "^2.0.1",
    "env-test": "^1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "event-stream": "^3.3.4",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "rewire": "^2.5.2",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "vinyl": "^1.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
