{
  "name": "gulp-i18next-conv",
  "version": "10.1.0",
  "description": "Gulp plugin to convert gettext .po or .mo files to i18next compatible JSON",
  "main": "dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=12.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "test": "mocha",
    "build": "rollup -c",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/gulp-i18next-conv.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "i18next",
    "gettext",
    "json"
  ],
  "author": "Julian Grinblat",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/dotcore64/gulp-i18next-conv/issues"
  },
  "homepage": "https://github.com/dotcore64/gulp-i18next-conv#readme",
  "dependencies": {
    "plugin-error": "^1.0.1",
    "through2": "^4.0.2",
    "vinyl-contents-tostring": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "babel-plugin-rewire": "^1.2.0",
    "babel-register-esm": "^1.2.1",
    "c8": "^7.11.2",
    "chai": "^4.3.6",
    "dirty-chai": "^2.0.1",
    "env-test": "^1.0.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-node": "^11.1.0",
    "event-stream": "^4.0.1",
    "i18next-conv": "^12.1.1",
    "mocha": "^9.2.2",
    "rollup": "^2.70.2",
    "sinon": "^13.0.2",
    "sinon-chai": "^3.7.0",
    "vinyl": "^2.2.1"
  },
  "peerDependencies": {
    "i18next-conv": ">=10.0.1"
  }
}
