{
  "name": "gulp-i18next-conv",
  "version": "12.0.0",
  "description": "Gulp plugin to convert gettext .po or .mo files to i18next compatible JSON",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "test": "mocha",
    "build": "babel src -s -d dist",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/gulp-i18next-conv.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "i18next",
    "gettext",
    "json"
  ],
  "author": "Julian Grinblat",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/dotcore64/gulp-i18next-conv/issues"
  },
  "homepage": "https://github.com/dotcore64/gulp-i18next-conv#readme",
  "dependencies": {
    "plugin-error": "^2.0.1",
    "through2": "^4.0.2",
    "vinyl-contents-tostring": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-throw-expressions": "^7.27.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "c8": "^10.1.3",
    "chai": "^4.5.0",
    "dirty-chai": "^2.0.1",
    "env-test": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "event-stream": "^4.0.1",
    "i18next-conv": "^15.1.1",
    "mocha": "^11.1.0",
    "sinon": "^20.0.0",
    "sinon-chai": "^3.7.0",
    "vinyl": "^3.0.0"
  },
  "peerDependencies": {
    "i18next-conv": ">=10.0.1"
  }
