{
  "name": "gulp-i18next-conv",
  "version": "8.0.1",
  "description": "Gulp plugin to convert gettext .po or .mo files to i18next compatible JSON",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint 'src/*.js' 'test/*.js'",
    "pretest": "npm run lint",
    "test": "mocha",
    "build": "babel src -d lib",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm run test && npm run build"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/gulp-i18next-conv.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "i18next",
    "gettext",
    "json"
  ],
  "author": "Julian Grinblat",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/dotcore64/gulp-i18next-conv/issues"
  },
  "homepage": "https://github.com/dotcore64/gulp-i18next-conv#readme",
  "dependencies": {
    "plugin-error": "^1.0.1",
    "through2": "^3.0.1",
    "vinyl-contents-tostring": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "dirty-chai": "^2.0.1",
    "env-test": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "esm": "^3.2.25",
    "event-stream": "^4.0.1",
    "i18next-conv": "^10.0.1",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "vinyl": "^2.2.0"
  },
  "peerDependencies": {
    "i18next-conv": ">=10.0.1"
  }
}
